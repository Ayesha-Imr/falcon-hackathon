{
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/greet",
      "dest": "backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
